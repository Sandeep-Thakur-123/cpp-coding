#include<iostream>
using namespace std;

void merge(int arr1[], int arr2[], int arr[], int n){
    
    int i = 0;
    int k = 0;
    int j = 0;
    int mid = n/2;
    int size1 = mid;
    int size2 = n-mid;
    
    while(i < size1 && j < size2){
        if(arr1[i] < arr2[j]){
            arr[k] = arr1[i];
            i++;
        }
        
        else{
            arr[k] = arr2[j];
            j++;
        }
        
        k++;
        
    }
    
    while(i < size1){
        arr[k] = arr1[i];
        i++;
        k++;
    }
    
    while(j < size2){
        arr[k] = arr2[j];
        j++;
        k++;
    }
    
    
}

void sort(int arr[], int n){
    
    for(int i = 0; i < n; i++){
        for(int j = 0; j < n-i-1; j++){
        
            if(arr[j+1] < arr[j]){
                int temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            }
        }
        
    }
}


void mergesort(int arr[], int n){
    
    int mid = n/2;
    
    int arr1[100];
    
    for(int i = 0; i < mid; i++){
        arr1[i] = arr[i];
    }
    
    sort(arr1, mid);
    
    int arr2[100];
    
    for(int i = mid; i < n; i++){
        arr2[i - mid] = arr[i];
    }
    
    sort(arr2, n-mid);
    
    merge(arr1, arr2, arr, n);
    
}

int main(){
    int n;
    cin >> n;
    
    int arr[100];
    
    for(int i = 0; i < n; i++){
        cin >> arr[i];
    }
    
    mergesort(arr,n);
    
    for(int i = 0; i < n; i++){
        cout << arr[i];
    }
}
