//Time Complexity O(nxn)

#include<iostream>
using namespace std;

string reversestring(string s, int n){
    
    string word = "";
    string result = "";
    
    for(int i = n-1; i >= 0; i--){
        
        if(s[i] != ' '){
            word = s[i] + word;
        }
        else{
            if(word.length() > 0)
            result = result + word + " ";
            word = "";
        }
    }
    
    // add last word
    result = result + word;
    
    return result;
}

int main(){
    string s;
    getline(cin,s);
    int n = s.length();
    cout << reversestring(s,n);
}

//Time Complexity O(n) - We can use dequeue also

#include<iostream>
#include<algorithm>
using namespace std;

string reversestring(string s, int n){
    
    string word = "";
    string result = "";
    
    for(int i = n-1; i >= 0; i--){
        
        if(s[i] != ' '){
            word += s[i];   // build reversed word
        }
        else{
            if(word.length() > 0){
                reverse(word.begin(), word.end());  // fix word
                result += word + " ";
                word = "";
            }
        }
    }
    
    if(word.length() > 0){
        reverse(word.begin(), word.end());
        result += word;
    }
    
    return result;
}
